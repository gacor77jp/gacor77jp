            <script>
                $('.btn-refresh-captcha').on('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    var $captchaImg = $(this).parent().find('img');
                    var curCapUrl = $captchaImg.attr("data-url");
                    var url = curCapUrl + Date.now() + Math.floor(Math.random() * 100000000);
                    $captchaImg.attr("src", url);

                });
            </script>
            
         
            
